<Alloy>
    <TableViewRow id="eventRow" onClick="doClickSurveyRow">
        <View class="eventContainer" id="eventContainer">
            <View class="eventTimeContainer" id="eventTimeContainer">
                <View class="eventTimeContent">
                    <Label id="eventTime" />
                    <Label id="eventTimeLabel" />
                </View>
            </View>

            <View class="eventInformation" id="eventInformation">
                <Label id="eventInformationTitle" />
                <Label id="eventInformationMessage" />
            </View>
        </View>
    </TableViewRow>
</Alloy>
